{
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "array-bracket-spacing":[2,"never"],
    "block-scoped-var": 2,
    "brace-style": [2, "1tbs"],
    "camelcase": 1,
    "comma-spacing": 2,
    "complexity": [2, 10],
    "computed-property-spacing":[2,"never"],
    "consistent-return": 0,
    "curly": 2,
    "dot-notation": 1,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "max-depth": [2, 3],
    "max-statements": [1, 15],
    "max-len": [1, 150],
    "max-nested-callbacks": [1, 4],
    "new-cap": 1,
    "no-console": 2,
    "no-extend-native": 2,
    "no-extra-semi": 1,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-shadow": 2,
    "no-sync": 2,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-use-before-define": [2, "nofunc"],
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-var": 2,
    "object-curly-spacing": [2,"never"],
    "prefer-const": 2,
    "quotes": [2, "single", "avoid-escape"],
    "require-jsdoc": 1,
    "semi": [2, "always"],
    "keyword-spacing": [2, { before: true, after: true}],
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "valid-jsdoc": [1, {
      "requireReturn": false,
      "prefer": {
        "arg": "param",
        "argument": "param",
        "constant": "const",
        "constructor": "class",
        "defaultvalue": "default",
        "desc": "description",
        "exception": "throws",
        "extends": "augments",
        "file": "overview",
        "fileoverview": "overview",
        "fires": "emits",
        "func": "function",
        "host": "external",
        "method": "function",
        "prop": "property",
        "return": "returns",
        "var": "member",
        "virtual": "abstract"
      }
    }]
  }
}